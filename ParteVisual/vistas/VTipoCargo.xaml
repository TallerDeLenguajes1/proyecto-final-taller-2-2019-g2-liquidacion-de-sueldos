        <Button x:Name="btnEliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="325,60,0,0" VerticalAlignment="Top" Width="75" Click="btnEliminar_Click"/>
        <ListBox HorizontalAlignment="Left" Height="399" Margin="405,10,0,0" VerticalAlignment="Top" Width="318"/>

