        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParteVisual.vistas"
        mc:Ignorable="d"
        Title="FAMTipoConcepto" Height="450" Width="800">
    <Grid>
        <Label Content="concepto:" HorizontalAlignment="Left" Margin="27,51,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.395,0.154"/>
        <Label Content="monto:" HorizontalAlignment="Left" Margin="27,82,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxConcepto" HorizontalAlignment="Left" Height="23" Margin="110,53,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbxmonto" HorizontalAlignment="Left" Height="23" Margin="110,85,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Button Content="btnAceptar" HorizontalAlignment="Left" Margin="89,125,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
